generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model PasswordResetToken {
  id        String   @id @default(cuid())
  email     String
  token     String
  expiresAt DateTime
  createdAt DateTime @default(now())
  
  @@unique([email, token])
  @@map("password_reset_tokens")
}

model User {
  id                 String                 @id @default(uuid())
  email              String                 @unique
  passwordHash       String                 @map("password_hash")
  createdAt          DateTime               @default(now()) @map("created_at")
  updatedAt          DateTime               @updatedAt @map("updated_at")
  lastLogin          DateTime?              @map("last_login")
  isActive           Boolean                @default(true) @map("is_active")
  emailVerified      Boolean                @default(false) @map("email_verified")
  auditLogs          AuditLog[]
  cplCreated         Cpl[]                  @relation("CplCreatedBy")
  cpmkCreated        Cpmk[]                 @relation("CpmkCreatedBy")
  evaluasiDosen      EvaluasiMataKuliah[]
  mkCreated          MataKuliah[]           @relation("MataKuliahCreatedBy")
  nilaiCreated       NilaiCpl[]             @relation("NilaiCreatedBy")
  nilaiTeknikCreated NilaiTeknikPenilaian[] @relation("NilaiTeknikCreatedBy")
  profile            Profile?
  sessions           Session[]
  role               UserRole?

  @@index([email])
  @@index([isActive])
  @@map("users")
}

model UserRole {
  id        Int      @id @default(autoincrement())
  userId    String   @unique @map("user_id")
  roleId    String   @map("role_id")
  role      Role     @relation(fields: [roleId], references: [id], onDelete: Restrict)
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([roleId])
  @@index([userId])
  @@map("user_roles")
}

model Profile {
  id                     String                   @id @default(uuid())
  userId                 String                   @unique @map("user_id")
  namaLengkap            String?                  @map("nama_lengkap")
  nim                    String?                  @unique
  nip                    String?                  @unique
  programStudi           String?                  @map("program_studi")
  semester               Int?
  tahunMasuk             Int?                     @map("tahun_masuk")
  alamat                 String?                  @db.Text
  noTelepon              String?                  @map("no_telepon")
  fotoProfile            String?                  @map("foto_profile")
  createdAt              DateTime                 @default(now()) @map("created_at")
  updatedAt              DateTime                 @updatedAt @map("updated_at")
  nidn                   String?                  @unique
  fakultasId             String?                  @map("fakultas_id")
  prodiId                String?                  @map("prodi_id")
  semesterId             String?                  @map("semester_id")
  kelasId                String?                  @map("kelas_id")
  angkatanId             String?                  @map("angkatan_id")
  mataKuliahPengampu     MataKuliahPengampu[]
  nilaiCpl               NilaiCpl[]               @relation("MahasiswaNilai")
  nilaiCpmk              NilaiCpmk[]              @relation("MahasiswaNilaiCpmk")
  nilaiSubCpmk           NilaiSubCpmk[]           @relation("MahasiswaNilaiSubCpmk")
  nilaiTeknik            NilaiTeknikPenilaian[]   @relation("MahasiswaNilaiTeknik")
  penilaianTidakLangsung PenilaianTidakLangsung[]
  angkatanRef            Angkatan?                @relation(fields: [angkatanId], references: [id])
  fakultas               Fakultas?                @relation(fields: [fakultasId], references: [id])
  kelasRef               Kelas?                   @relation(fields: [kelasId], references: [id])
  prodi                  Prodi?                   @relation(fields: [prodiId], references: [id])
  semesterRef            Semester?                @relation(fields: [semesterId], references: [id])
  user                   User                     @relation(fields: [userId], references: [id], onDelete: Cascade)
  krs                    Krs[]

  @@index([nim])
  @@index([nip])
  @@index([nidn])
  @@index([programStudi])
  @@index([prodiId])
  @@index([fakultasId])
  @@index([semester])
  @@index([semesterId])
  @@index([kelasId])
  @@index([angkatanId], map: "profiles_angkatan_id_fkey")
  @@map("profiles")
}

model Cpl {
  id                     String                   @id @default(uuid())
  kodeCpl                String                   @unique @map("kode_cpl")
  deskripsi              String                   @db.Text
  kategori               String?
  isActive               Boolean                  @default(true) @map("is_active")
  createdAt              DateTime                 @default(now()) @map("created_at")
  updatedAt              DateTime                 @updatedAt @map("updated_at")
  createdBy              String?                  @map("created_by")
  kategoriId             String?                  @map("kategori_id")
  prodiId                String?                  @map("prodi_id")
  creator                User?                    @relation("CplCreatedBy", fields: [createdBy], references: [id])
  kategoriRef            KategoriCpl?             @relation(fields: [kategoriId], references: [id])
  prodi                  Prodi?                   @relation(fields: [prodiId], references: [id])
  mataKuliah             CplMataKuliah[]
  cpmkMappings           CpmkCplMapping[]
  nilaiCpl               NilaiCpl[]
  penilaianTidakLangsung PenilaianTidakLangsung[]
  profilLulusanMappings  ProfilLulusanCpl[]
  targetCPL              TargetCPL[]
  tindakLanjutCPL        TindakLanjutCPL[]
  kurikulumId            String?                  @map("kurikulum_id")
  kurikulum              Kurikulum?               @relation(fields: [kurikulumId], references: [id])

  @@index([kodeCpl])
  @@index([kategori])
  @@index([kategoriId])
  @@index([prodiId])
  @@index([isActive])
  @@index([kurikulumId])
  @@index([createdBy], map: "cpl_created_by_fkey")
  @@map("cpl")
}

model MataKuliah {
  id           String                 @id @default(uuid())
  kodeMk       String                 @unique @map("kode_mk")
  namaMk       String                 @map("nama_mk")
  sks          Int
  semester     Int
  deskripsi    String?                @db.Text
  isActive     Boolean                @default(true) @map("is_active")
  createdAt    DateTime               @default(now()) @map("created_at")
  updatedAt    DateTime               @updatedAt @map("updated_at")
  createdBy    String?                @map("created_by")
  programStudi String?                @map("program_studi")
  jenisMkId    String?                @map("jenis_mk_id")
  kurikulumId  String?                @map("kurikulum_id")
  prodiId      String?                @map("prodi_id")
  semesterId   String?                @map("semester_id")
  cpl          CplMataKuliah[]
  cpmk         Cpmk[]
  evaluasi     EvaluasiMataKuliah[]
  creator      User?                  @relation("MataKuliahCreatedBy", fields: [createdBy], references: [id])
  jenisMk      JenisMataKuliah?       @relation(fields: [jenisMkId], references: [id])
  kurikulum    Kurikulum?             @relation(fields: [kurikulumId], references: [id])
  prodi        Prodi?                 @relation(fields: [prodiId], references: [id])
  semesterRef  Semester?              @relation(fields: [semesterId], references: [id])
  pengampu     MataKuliahPengampu[]
  nilaiCpl     NilaiCpl[]
  nilaiCpmk    NilaiCpmk[]
  nilaiSubCpmk NilaiSubCpmk[]
  nilaiTeknik  NilaiTeknikPenilaian[]
  krs          Krs[]

  @@index([kodeMk])
  @@index([semester])
  @@index([semesterId])
  @@index([programStudi])
  @@index([prodiId])
  @@index([kurikulumId])
  @@index([isActive])
  @@index([createdBy], map: "mata_kuliah_created_by_fkey")
  @@index([jenisMkId], map: "mata_kuliah_jenis_mk_id_fkey")
  @@map("mata_kuliah")
}

model CplMataKuliah {
  id              String     @id @default(uuid())
  cplId           String     @map("cpl_id")
  mataKuliahId    String     @map("mata_kuliah_id")
  bobotKontribusi Decimal    @default(1.00) @map("bobot_kontribusi") @db.Decimal(3, 2)
  createdAt       DateTime   @default(now()) @map("created_at")
  updatedAt       DateTime   @updatedAt @map("updated_at")
  cpl             Cpl        @relation(fields: [cplId], references: [id], onDelete: Cascade)
  mataKuliah      MataKuliah @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)

  @@unique([cplId, mataKuliahId])
  @@index([cplId])
  @@index([mataKuliahId])
  @@map("cpl_mata_kuliah")
}

model Cpmk {
  id                String            @id @default(uuid())
  kodeCpmk          String            @map("kode_cpmk")
  deskripsi         String?           @db.Text
  mataKuliahId      String            @map("mata_kuliah_id")
  isActive          Boolean           @default(true) @map("is_active")
  createdAt         DateTime          @default(now()) @map("created_at")
  updatedAt         DateTime          @updatedAt @map("updated_at")
  createdBy         String?           @map("created_by")
  statusValidasi    String            @default("draft") @map("status_validasi")
  validatedAt       DateTime?         @map("validated_at")
  validatedBy       String?           @map("validated_by")
  levelTaksonomi    String?           @map("level_taksonomi")
  levelTaksonomiId  String?           @map("level_taksonomi_id")
  creator           User?             @relation("CpmkCreatedBy", fields: [createdBy], references: [id])
  levelTaksonomiRef LevelTaksonomi?   @relation(fields: [levelTaksonomiId], references: [id])
  mataKuliah        MataKuliah        @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  cplMappings       CpmkCplMapping[]
  nilaiCpmk         NilaiCpmk[]
  rubrik            Rubrik?
  subCpmk           SubCpmk[]
  teknikPenilaian   TeknikPenilaian[]

  @@index([mataKuliahId])
  @@index([kodeCpmk])
  @@index([statusValidasi])
  @@index([levelTaksonomiId])
  @@index([isActive])
  @@index([createdBy], map: "cpmk_created_by_fkey")
  @@map("cpmk")
}

model CpmkCplMapping {
  id              String   @id @default(uuid())
  cpmkId          String   @map("cpmk_id")
  cplId           String   @map("cpl_id")
  bobotPersentase Decimal  @map("bobot_persentase") @db.Decimal(5, 2)
  createdAt       DateTime @default(now()) @map("created_at")
  updatedAt       DateTime @updatedAt @map("updated_at")
  cpl             Cpl      @relation(fields: [cplId], references: [id], onDelete: Cascade)
  cpmk            Cpmk     @relation(fields: [cpmkId], references: [id], onDelete: Cascade)

  @@unique([cpmkId, cplId])
  @@index([cpmkId])
  @@index([cplId])
  @@map("cpmk_cpl_mapping")
}

model TeknikPenilaian {
  id              String                 @id @default(uuid())
  cpmkId          String                 @map("cpmk_id")
  namaTeknik      String                 @map("nama_teknik")
  bobotPersentase Decimal                @map("bobot_persentase") @db.Decimal(5, 2)
  deskripsi       String?                @db.Text
  createdAt       DateTime               @default(now()) @map("created_at")
  updatedAt       DateTime               @updatedAt @map("updated_at")
  teknikRefId     String?                @map("teknik_ref_id")
  subCpmkMappings AsesmenSubCpmk[]
  nilaiTeknik     NilaiTeknikPenilaian[]
  cpmk            Cpmk                   @relation(fields: [cpmkId], references: [id], onDelete: Cascade)
  teknikRef       TeknikPenilaianRef?    @relation(fields: [teknikRefId], references: [id])

  @@index([cpmkId])
  @@index([teknikRefId])
  @@map("teknik_penilaian")
}

model SubCpmk {
  id              String           @id @default(uuid())
  cpmkId          String           @map("cpmk_id")
  kode            String
  deskripsi       String           @db.Text
  bobot           Decimal          @default(0.00) @db.Decimal(5, 2)
  createdAt       DateTime         @default(now()) @map("created_at")
  updatedAt       DateTime         @updatedAt @map("updated_at")
  asesmenMappings AsesmenSubCpmk[]
  nilaiSubCpmk    NilaiSubCpmk[]
  cpmk            Cpmk             @relation(fields: [cpmkId], references: [id], onDelete: Cascade)

  @@index([cpmkId])
  @@map("sub_cpmk")
}

model AsesmenSubCpmk {
  id                String          @id @default(uuid())
  teknikPenilaianId String          @map("teknik_penilaian_id")
  subCpmkId         String          @map("sub_cpmk_id")
  bobot             Decimal         @default(100.00) @db.Decimal(5, 2)
  createdAt         DateTime        @default(now()) @map("created_at")
  subCpmk           SubCpmk         @relation(fields: [subCpmkId], references: [id], onDelete: Cascade)
  teknikPenilaian   TeknikPenilaian @relation(fields: [teknikPenilaianId], references: [id], onDelete: Cascade)

  @@unique([teknikPenilaianId, subCpmkId])
  @@index([teknikPenilaianId])
  @@index([subCpmkId])
  @@map("asesmen_sub_cpmk")
}

model NilaiCpl {
  id           String     @id @default(uuid())
  mahasiswaId  String     @map("mahasiswa_id")
  cplId        String     @map("cpl_id")
  mataKuliahId String     @map("mata_kuliah_id")
  nilai        Decimal    @db.Decimal(5, 2)
  semester     Int
  semesterId    String?      @map("semester_id")
  tahunAjaranId String?       @map("tahun_ajaran_id")
  catatan       String?      @db.Text
  createdAt     DateTime     @default(now()) @map("created_at")
  updatedAt     DateTime     @updatedAt @map("updated_at")
  createdBy     String?      @map("created_by")
  cpl           Cpl          @relation(fields: [cplId], references: [id], onDelete: Cascade)
  creator       User?        @relation("NilaiCreatedBy", fields: [createdBy], references: [id])
  mahasiswa     Profile      @relation("MahasiswaNilai", fields: [mahasiswaId], references: [userId], onDelete: Cascade)
  mataKuliah    MataKuliah   @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  semesterRef   Semester?    @relation(fields: [semesterId], references: [id])
  tahunAjaranRef TahunAjaran? @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mahasiswaId, cplId, mataKuliahId, semester, tahunAjaranId])
  @@index([mahasiswaId])
  @@index([cplId])
  @@index([mataKuliahId])
  @@index([semester])
  @@index([semesterId])
  @@index([tahunAjaranId])
  @@index([createdBy], map: "nilai_cpl_created_by_fkey")
  @@map("nilai_cpl")
}

model NilaiTeknikPenilaian {
  id                String          @id @default(uuid())
  mahasiswaId       String          @map("mahasiswa_id")
  teknikPenilaianId String          @map("teknik_penilaian_id")
  mataKuliahId      String          @map("mata_kuliah_id")
  nilai             Decimal         @db.Decimal(5, 2)
  semester          Int
  semesterId        String?         @map("semester_id")
  tahunAjaranId     String?          @map("tahun_ajaran_id")
  catatan           String?         @db.Text
  createdAt         DateTime        @default(now()) @map("created_at")
  updatedAt         DateTime        @updatedAt @map("updated_at")
  createdBy         String?         @map("created_by")
  nilaiRubrik       NilaiRubrik[]
  creator           User?           @relation("NilaiTeknikCreatedBy", fields: [createdBy], references: [id])
  mahasiswa         Profile         @relation("MahasiswaNilaiTeknik", fields: [mahasiswaId], references: [userId], onDelete: Cascade)
  mataKuliah        MataKuliah      @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  semesterRef       Semester?       @relation(fields: [semesterId], references: [id])
  teknikPenilaian   TeknikPenilaian @relation(fields: [teknikPenilaianId], references: [id], onDelete: Cascade)
  tahunAjaranRef    TahunAjaran?     @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mahasiswaId, teknikPenilaianId, semester, tahunAjaranId])
  @@index([mahasiswaId])
  @@index([teknikPenilaianId])
  @@index([mataKuliahId])
  @@index([semester])
  @@index([semesterId])
  @@index([tahunAjaranId])
  @@index([createdBy], map: "nilai_teknik_penilaian_created_by_fkey")
  @@map("nilai_teknik_penilaian")
}

model NilaiSubCpmk {
  id           String     @id @default(uuid())
  mahasiswaId  String     @map("mahasiswa_id")
  subCpmkId    String     @map("sub_cpmk_id")
  mataKuliahId String     @map("mata_kuliah_id")
  nilai        Decimal    @db.Decimal(5, 2)
  semester     Int
  semesterId   String?    @map("semester_id")
  tahunAjaranId String?    @map("tahun_ajaran_id")
  createdAt    DateTime   @default(now()) @map("created_at")
  updatedAt    DateTime   @updatedAt @map("updated_at")
  mahasiswa    Profile    @relation("MahasiswaNilaiSubCpmk", fields: [mahasiswaId], references: [userId], onDelete: Cascade)
  mataKuliah   MataKuliah @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  semesterRef  Semester?  @relation(fields: [semesterId], references: [id])
  subCpmk      SubCpmk    @relation(fields: [subCpmkId], references: [id], onDelete: Cascade)
  tahunAjaranRef TahunAjaran? @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mahasiswaId, subCpmkId, semester, tahunAjaranId])
  @@index([mahasiswaId])
  @@index([subCpmkId])
  @@index([mataKuliahId])
  @@index([semester])
  @@index([semesterId])
  @@index([tahunAjaranId])
  @@map("nilai_sub_cpmk")
}

model NilaiCpmk {
  id           String     @id @default(uuid())
  mahasiswaId  String     @map("mahasiswa_id")
  cpmkId       String     @map("cpmk_id")
  mataKuliahId String     @map("mata_kuliah_id")
  nilaiAkhir   Decimal    @map("nilai_akhir") @db.Decimal(5, 2)
  semester     Int
  semesterId   String?    @map("semester_id")
  tahunAjaranId String?    @map("tahun_ajaran_id")
  isCalculated Boolean    @default(true) @map("is_calculated")
  calculatedAt DateTime   @default(now()) @map("calculated_at")
  updatedAt    DateTime   @updatedAt @map("updated_at")
  cpmk         Cpmk       @relation(fields: [cpmkId], references: [id], onDelete: Cascade)
  mahasiswa    Profile    @relation("MahasiswaNilaiCpmk", fields: [mahasiswaId], references: [userId], onDelete: Cascade)
  mataKuliah   MataKuliah @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  semesterRef  Semester?  @relation(fields: [semesterId], references: [id])
  tahunAjaranRef TahunAjaran? @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mahasiswaId, cpmkId, semester, tahunAjaranId])
  @@index([mahasiswaId])
  @@index([cpmkId])
  @@index([mataKuliahId])
  @@index([semester])
  @@index([semesterId])
  @@index([tahunAjaranId])
  @@map("nilai_cpmk")
}

model Session {
  id        String   @id @default(uuid())
  userId    String   @map("user_id")
  token     String   @db.VarChar(1000)
  expiresAt DateTime @map("expires_at")
  createdAt DateTime @default(now()) @map("created_at")
  ipAddress String?  @map("ip_address")
  userAgent String?  @map("user_agent") @db.Text
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId])
  @@index([token(length: 768)])
  @@index([expiresAt])
  @@map("sessions")
}

model AuditLog {
  id        BigInt   @id @default(autoincrement())
  userId    String?  @map("user_id")
  action    String
  tableName String   @map("table_name")
  recordId  String?  @map("record_id")
  oldData   String?  @map("old_data") @db.LongText
  newData   String?  @map("new_data") @db.LongText
  ipAddress String?  @map("ip_address")
  userAgent String?  @map("user_agent") @db.Text
  createdAt DateTime @default(now()) @map("created_at")
  user      User?    @relation(fields: [userId], references: [id])

  @@index([userId])
  @@index([action])
  @@index([tableName])
  @@index([createdAt])
  @@map("audit_logs")
}


model Jenjang {
  id        String   @id @default(uuid())
  nama      String   @unique // e.g., D3, S1, S2
  keterangan String? @db.Text
  isActive  Boolean  @default(true) @map("is_active")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("jenjang")
}

model Fakultas {
  id        String    @id @default(uuid())
  nama      String    @unique
  kode      String    @unique
  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @updatedAt @map("updated_at")
  prodi     Prodi[]
  profiles  Profile[]

  @@map("fakultas")
}

model Prodi {
  id            String          @id @default(uuid())
  fakultasId    String          @map("fakultas_id")
  nama          String          @unique
  kode          String?
  jenjang       String?
  createdAt     DateTime        @default(now()) @map("created_at")
  updatedAt     DateTime        @updatedAt @map("updated_at")
  cpl           Cpl[]
  kaprodiData   KaprodiData?
  mataKuliah    MataKuliah[]
  fakultas      Fakultas        @relation(fields: [fakultasId], references: [id], onDelete: Cascade)
  profilLulusan ProfilLulusan[]
  profiles      Profile[]
  visiMisi      VisiMisi[]

  @@index([fakultasId])
  @@map("prodi")
}

model TahunAjaran {
  id        String   @id @default(uuid())
  nama      String   @unique
  isActive  Boolean  @default(false) @map("is_active")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  nilaiCpl               NilaiCpl[]
  nilaiTeknik            NilaiTeknikPenilaian[]
  nilaiSubCpmk           NilaiSubCpmk[]
  nilaiCpmk              NilaiCpmk[]
  evaluasiMataKuliah     EvaluasiMataKuliah[]
  penilaianTidakLangsung PenilaianTidakLangsung[]
  krs                     Krs[]

  @@map("tahun_ajaran")
}

model KategoriCpl {
  id        String   @id @default(uuid())
  nama      String   @unique
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  cpl       Cpl[]

  @@map("kategori_cpl")
}

model TeknikPenilaianRef {
  id              String            @id @default(uuid())
  nama            String            @unique
  deskripsi       String?           @db.Text
  createdAt       DateTime          @default(now()) @map("created_at")
  updatedAt       DateTime          @updatedAt @map("updated_at")
  teknikPenilaian TeknikPenilaian[]

  @@map("teknik_penilaian_ref")
}

model LevelTaksonomi {
  id        String   @id @default(uuid())
  kode      String   @unique
  deskripsi String?  @db.Text
  kategori  String?
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  cpmk      Cpmk[]

  @@map("level_taksonomi")
}

model Kurikulum {
  id           String       @id @default(uuid())
  nama         String       @unique
  tahunMulai   Int          @map("tahun_mulai")
  tahunSelesai Int?         @map("tahun_selesai")
  isActive     Boolean      @default(false) @map("is_active")
  createdAt    DateTime     @default(now()) @map("created_at")
  updatedAt    DateTime     @updatedAt @map("updated_at")
  mataKuliah   MataKuliah[]
  cpl          Cpl[]
  profilLulusan ProfilLulusan[]
  angkatan     Angkatan[]

  @@map("kurikulum")
}

model JenisMataKuliah {
  id         String       @id @default(uuid())
  nama       String       @unique
  createdAt  DateTime     @default(now()) @map("created_at")
  updatedAt  DateTime     @updatedAt @map("updated_at")
  mataKuliah MataKuliah[]

  @@map("jenis_mata_kuliah")
}

model MataKuliahPengampu {
  id           String     @id @default(uuid())
  mataKuliahId String     @map("mata_kuliah_id")
  dosenId      String     @map("dosen_id")
  kelasId      String?    @map("kelas_id")
  isPengampu   Boolean    @default(true) @map("is_pengampu_utama")
  createdAt    DateTime   @default(now()) @map("created_at")
  updatedAt    DateTime   @updatedAt @map("updated_at")
  dosen        Profile    @relation(fields: [dosenId], references: [userId], onDelete: Cascade)
  kelas        Kelas?     @relation(fields: [kelasId], references: [id])
  mataKuliah   MataKuliah @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)

  @@unique([mataKuliahId, dosenId, kelasId])
  @@index([mataKuliahId])
  @@index([dosenId])
  @@index([kelasId])
  @@map("mata_kuliah_pengampu")
}

model KaprodiData {
  id           String   @id @default(uuid())
  programStudi String   @unique @map("program_studi")
  namaKaprodi  String   @map("nama_kaprodi")
  nidnKaprodi  String   @map("nidn_kaprodi")
  createdAt    DateTime @default(now()) @map("created_at")
  updatedAt    DateTime @updatedAt @map("updated_at")
  prodiId      String?  @unique @map("prodi_id")
  prodi        Prodi?   @relation(fields: [prodiId], references: [id])

  @@index([programStudi])
  @@index([prodiId])
  @@map("kaprodi_data")
}

model Settings {
  id          String   @id @default(uuid())
  key         String   @unique
  value       String   @db.Text
  description String?  @db.Text
  updatedAt   DateTime @updatedAt @map("updated_at")

  @@map("settings")
}

model Semester {
  id           String                 @id @default(uuid())
  nama         String                 @unique
  angka        Int                    @unique
  isActive     Boolean                @default(true) @map("is_active")
  createdAt    DateTime               @default(now()) @map("created_at")
  updatedAt    DateTime               @updatedAt @map("updated_at")
  mataKuliah   MataKuliah[]
  nilaiCpl     NilaiCpl[]
  nilaiCpmk    NilaiCpmk[]
  nilaiSubCpmk NilaiSubCpmk[]
  nilaiTeknik  NilaiTeknikPenilaian[]
  profiles     Profile[]
  krs          Krs[]

  @@map("semester")
}

model Kelas {
  id        String               @id @default(uuid())
  nama      String               @unique
  createdAt DateTime             @default(now()) @map("created_at")
  updatedAt DateTime             @updatedAt @map("updated_at")
  pengampu  MataKuliahPengampu[]
  profiles  Profile[]

  @@map("kelas")
}

model Angkatan {
  id        String    @id @default(uuid())
  tahun     Int       @unique
  isActive  Boolean   @default(true) @map("is_active")
  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @updatedAt @map("updated_at")
  profiles  Profile[]
  kurikulumId String?   @map("kurikulum_id")
  kurikulum   Kurikulum? @relation(fields: [kurikulumId], references: [id])

  @@map("angkatan")
  @@index([kurikulumId])
}

model Rubrik {
  id        String           @id @default(uuid())
  cpmkId    String           @unique @map("cpmk_id")
  deskripsi String?          @db.Text
  isActive  Boolean          @default(true) @map("is_active")
  createdAt DateTime         @default(now()) @map("created_at")
  updatedAt DateTime         @updatedAt @map("updated_at")
  cpmk      Cpmk             @relation(fields: [cpmkId], references: [id], onDelete: Cascade)
  kriteria  RubrikKriteria[]

  @@index([cpmkId])
  @@map("rubrik")
}

model RubrikKriteria {
  id        String        @id @default(uuid())
  rubrikId  String        @map("rubrik_id")
  deskripsi String        @db.Text
  bobot     Decimal       @default(1.00) @db.Decimal(5, 2)
  createdAt DateTime      @default(now()) @map("created_at")
  updatedAt DateTime      @updatedAt @map("updated_at")
  rubrik    Rubrik        @relation(fields: [rubrikId], references: [id], onDelete: Cascade)
  levels    RubrikLevel[]

  @@index([rubrikId])
  @@map("rubrik_kriteria")
}

model RubrikLevel {
  id          String         @id @default(uuid())
  kriteriaId  String         @map("kriteria_id")
  deskripsi   String         @db.Text
  nilai       Decimal        @db.Decimal(5, 2)
  label       String?
  createdAt   DateTime       @default(now()) @map("created_at")
  updatedAt   DateTime       @updatedAt @map("updated_at")
  nilaiRubrik NilaiRubrik[]
  kriteria    RubrikKriteria @relation(fields: [kriteriaId], references: [id], onDelete: Cascade)

  @@index([kriteriaId])
  @@map("rubrik_level")
}

model NilaiRubrik {
  id            String               @id @default(uuid())
  nilaiTeknikId String               @map("nilai_teknik_id")
  rubrikLevelId String               @map("rubrik_level_id")
  createdAt     DateTime             @default(now()) @map("created_at")
  updatedAt     DateTime             @updatedAt @map("updated_at")
  nilaiTeknik   NilaiTeknikPenilaian @relation(fields: [nilaiTeknikId], references: [id], onDelete: Cascade)
  rubrikLevel   RubrikLevel          @relation(fields: [rubrikLevelId], references: [id], onDelete: Cascade)

  @@unique([nilaiTeknikId, rubrikLevelId])
  @@index([nilaiTeknikId])
  @@index([rubrikLevelId])
  @@map("nilai_rubrik")
}

model EvaluasiMataKuliah {
  id               String     @id @default(uuid())
  mataKuliahId     String     @map("mata_kuliah_id")
  dosenId          String     @map("dosen_id")
  semester         Int
  tahunAjaranId    String?     @map("tahun_ajaran_id")
  kendala          String?    @db.Text
  rencanaPerbaikan String?    @map("rencana_perbaikan") @db.Text
  status           String     @default("submitted")
  feedbackKaprodi  String?    @map("feedback_kaprodi") @db.Text
  createdAt        DateTime   @default(now()) @map("created_at")
  updatedAt        DateTime   @updatedAt @map("updated_at")
  dosen            User       @relation(fields: [dosenId], references: [id])
  mataKuliah       MataKuliah @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  tahunAjaranRef   TahunAjaran? @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mataKuliahId, dosenId, semester, tahunAjaranId])
  @@index([mataKuliahId])
  @@index([dosenId])
  @@index([tahunAjaranId])
  @@map("evaluasi_mata_kuliah")
}

model VisiMisi {
  id        String   @id @default(uuid())
  prodiId   String   @map("prodi_id")
  urutan    Int      @default(1)
  teks      String   @db.Text
  tipe      String
  isActive  Boolean  @default(true) @map("is_active")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  prodi     Prodi    @relation(fields: [prodiId], references: [id], onDelete: Cascade)

  @@index([prodiId])
  @@map("visi_misi")
}

model ProfilLulusan {
  id          String             @id @default(uuid())
  prodiId     String             @map("prodi_id")
  kode        String
  nama        String
  deskripsi          String?            @db.Text
  targetKetercapaian Float?             @map("target_ketercapaian")
  isActive           Boolean            @default(true) @map("is_active")
  createdAt   DateTime           @default(now()) @map("created_at")
  updatedAt   DateTime           @updatedAt @map("updated_at")
  prodi       Prodi              @relation(fields: [prodiId], references: [id], onDelete: Cascade)
  cplMappings ProfilLulusanCpl[]
  kurikulumId String?            @map("kurikulum_id")
  kurikulum   Kurikulum?         @relation(fields: [kurikulumId], references: [id])

  @@unique([prodiId, kode])
  @@index([prodiId])
  @@index([kurikulumId])
  @@map("profil_lulusan")
}

model ProfilLulusanCpl {
  id              String        @id @default(uuid())
  profilLulusanId String        @map("profil_lulusan_id")
  cplId           String        @map("cpl_id")
  createdAt       DateTime      @default(now()) @map("created_at")
  cpl             Cpl           @relation(fields: [cplId], references: [id], onDelete: Cascade)
  profilLulusan   ProfilLulusan @relation(fields: [profilLulusanId], references: [id], onDelete: Cascade)

  @@index([cplId])
  @@index([profilLulusanId], map: "profil_lulusan_cpl_profil_lulusan_id_fkey")
  @@map("profil_lulusan_cpl")
}

model PenilaianTidakLangsung {
  id          String   @id @default(uuid())
  mahasiswaId String   @map("mahasiswa_id")
  cplId       String   @map("cpl_id")
  nilai       Int
  semester    Int
  tahunAjaranId String?   @map("tahun_ajaran_id")
  createdAt     DateTime @default(now()) @map("created_at")
  updatedAt     DateTime @updatedAt @map("updated_at")
  cpl           Cpl      @relation(fields: [cplId], references: [id], onDelete: Cascade)
  mahasiswa     Profile  @relation(fields: [mahasiswaId], references: [userId], onDelete: Cascade)
  tahunAjaranRef TahunAjaran? @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mahasiswaId, cplId, semester, tahunAjaranId])
  @@index([mahasiswaId])
  @@index([cplId])
  @@index([tahunAjaranId])
  @@map("penilaian_tidak_langsung")
}

model RolePermission {
  id        String   @id @default(uuid())
  roleId    String   @map("role_id")
  role      Role     @relation(fields: [roleId], references: [id], onDelete: Cascade)
  resource  String
  action    String
  isEnabled Boolean  @default(false) @map("is_enabled")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@unique([roleId, resource, action])
  @@map("role_permissions")
}

model DefaultRolePermission {
  id        String   @id @default(uuid())
  roleId    String   @map("role_id")
  role      Role     @relation(fields: [roleId], references: [id], onDelete: Cascade)
  resource  String
  action    String
  isEnabled Boolean  @default(true) @map("is_enabled")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@unique([roleId, resource, action])
  @@map("default_role_permissions")
}

model TargetCPL {
  id          String   @id @default(uuid())
  prodiId     String   @map("prodi_id")
  angkatan    String
  tahunAjaran String   @map("tahun_ajaran")
  semester    Int?
  cplId       String   @map("cpl_id")
  target      Float
  createdAt   DateTime @default(now()) @map("created_at")
  updatedAt   DateTime @updatedAt @map("updated_at")
  cpl         Cpl      @relation(fields: [cplId], references: [id], onDelete: Cascade)

  @@unique([prodiId, angkatan, tahunAjaran, semester, cplId], name: "unique_target")
  @@index([cplId], map: "target_cpl_cpl_id_fkey")
  @@map("target_cpl")
}

model TindakLanjutCPL {
  id               String   @id @default(uuid())
  evaluasiId       String?  @map("evaluasi_id")
  prodiId          String   @map("prodi_id")
  angkatan         String
  tahunAjaran      String   @map("tahun_ajaran")
  semester         Int?
  cplId            String   @map("cpl_id")
  akarMasalah      String   @map("akar_masalah") @db.Text
  rencanaPerbaikan String   @map("rencana_perbaikan") @db.Text
  penanggungJawab  String   @map("penanggung_jawab")
  targetSemester   String   @map("target_semester")
  status           String   @default("open")
  createdAt        DateTime @default(now()) @map("created_at")
  updatedAt        DateTime @updatedAt @map("updated_at")
  createdBy        String   @map("created_by")
  cpl              Cpl      @relation(fields: [cplId], references: [id], onDelete: Cascade)

  @@index([prodiId, angkatan, tahunAjaran, cplId])
  @@index([cplId], map: "tindak_lanjut_cpl_cpl_id_fkey")
  @@map("tindak_lanjut_cpl")
}

model Role {
  id          String   @id @default(uuid())
  name        String   @unique
  displayName String   @map("display_name")
  description String?  @db.Text
  isSystem    Boolean  @default(false) @map("is_system")
  isActive    Boolean  @default(true) @map("is_active")
  icon        String?
  color       String?
  sortOrder   Int      @default(0) @map("sort_order")
  createdAt   DateTime @default(now()) @map("created_at")
  updatedAt   DateTime @updatedAt @map("updated_at")

  userRoles            UserRole[]
  rolePermissions      RolePermission[]
  defaultPermissions   DefaultRolePermission[]

  @@map("roles")
}

model SkalaNilai {
  id        String   @id @default(uuid())
  huruf     String   @unique
  nilaiMin  Float    @map("nilai_min")
  nilaiMax  Float    @map("nilai_max")
  isLulus   Boolean  @default(true) @map("is_lulus")
  isSystem  Boolean  @default(true) @map("is_system")
  isActive  Boolean  @default(true) @map("is_active")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("skala_nilai")
}

model Krs {
  id            String      @id @default(uuid())
  mahasiswaId   String      @map("mahasiswa_id")
  mataKuliahId  String      @map("mata_kuliah_id")
  semesterId    String      @map("semester_id")
  tahunAjaranId String      @map("tahun_ajaran_id")
  createdAt     DateTime    @default(now()) @map("created_at")
  updatedAt     DateTime    @updatedAt @map("updated_at")
  
  mahasiswa     Profile     @relation(fields: [mahasiswaId], references: [userId], onDelete: Cascade)
  mataKuliah    MataKuliah  @relation(fields: [mataKuliahId], references: [id], onDelete: Cascade)
  semester      Semester    @relation(fields: [semesterId], references: [id])
  tahunAjaran   TahunAjaran @relation(fields: [tahunAjaranId], references: [id])

  @@unique([mahasiswaId, mataKuliahId, semesterId, tahunAjaranId])
  @@index([mahasiswaId])
  @@index([mataKuliahId])
  @@index([semesterId])
  @@index([tahunAjaranId])
  @@map("krs")
}
